<div class="form-group row">
    <label class="col-form-label col-sm-1" for="name">제목</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="name" value='<%= model.name %>' >
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-1" for="author">저자</label>
    <div class="col-sm-8">
      <input class="form-control" name="author" placeholder="<%= model.author %>" value="<%= model.author %>" >
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-1" for="genre">장르</label>
    <div class="col-sm-8">
      <input class="form-control" name="genre" value="<%= model.genre %>">
    </div>
</div>
